{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
        "title": {
            "type": "string",
            "minLength": 3,
            "maxLength": 75
        },
        "description": {
            "type": "string"
        },
        "img_url": {
            "type": "string"
        },
        "price": {
            "type": "number"
        },
        "is_digital": {
            "type": "boolean"
        }
    },
    "additionalProperties": false,
    "required": ["title", "description", "img_url", "price"]
}
